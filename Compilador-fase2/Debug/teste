{funcoes}
funcao funcEx (a, b : numerico): numerico;
inicio
se b=0 entao entao funcEx := a
senao funEx := b
fim; 

funcao funcEx (a,b : numerico): numerico;
inicio
se b=0 entao funcEx := a
senao funEx := b
fim ;

{inicio programa}
inicio
x := ;
y := 2;
funEx(x,y+1);
a := (x+y)
fim.